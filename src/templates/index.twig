{% extends 'layouts/default.twig' %}
{% set body_class = 'index' %}
{% block page %}
			<section class="cover">
				<div class="cover-area">
					<div class="wrapper">
						<img class="image-background show-mobile" data-aos="fade-in" src="assets/img/graphics/mobile/cover_bg-mobile.png" alt="">
						<img class="progressive-image show-desktop image-background" data-aos="fade-in" src="assets/img/graphics/mobile/cover_bg.png" img-mobile="assets/img/graphics/mobile/cover_bg.png" img-tablet="assets/img/graphics/tablet/cover_bg.png" img-full="assets/img/graphics/full/cover_bg.png" alt="">
						<div class="image-inside">
							<div class="boat" data-aos="fade-left" data-aos-delay="600">
								<img class="show-mobile" src="assets/img/graphics/mobile/cover_boat-mobile.png" alt="">
								<img class="show-desktop" src="assets/img/graphics/full/cover_boat.png" alt="">
								<button type="button" class="trigger-modal hover-action" data-toggle="modal" data-target="#modal-boat"></button>
							</div>
							<div class="motor" data-aos="fade-right" data-aos-delay="600">
								<img class="show-mobile" src="assets/img/graphics/mobile/cover_motor-mobile.png" alt="">
								<img class="show-desktop" src="assets/img/graphics/full/cover_motor.png" alt="">
								<button type="button" class="trigger-modal hover-action" data-toggle="modal" data-target="#modal-motor"></button>
							</div>
							<div class="car" data-aos="fade-down-right" data-aos-delay="600">
								<img class="show-mobile" src="assets/img/graphics/mobile/cover_car-mobile.png" alt="">
								<img class="show-desktop" src="assets/img/graphics/full/cover_car.png" alt="">
								<button type="button" class="trigger-modal hover-action" data-toggle="modal" data-target="#modal-car"></button>
							</div>
						</div>
						<div class="image-stars" data-aos="fade-in" data-aos-delay="900">
							<img class="image-floating one show-mobile " src="assets/img/graphics/mobile/cover_star1-mobile.png" alt="">
							<img class="progressive-image image-floating one show-desktop" src="assets/img/graphics/mobile/cover_star1.png" img-mobile="assets/img/graphics/mobile/cover_star1.png" img-tablet="assets/img/graphics/tablet/cover_star1.png" img-full="assets/img/graphics/full/cover_star1.png" alt="">
